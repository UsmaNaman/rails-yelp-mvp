<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
    <h1>🍛 - Restaurants: - 🍝</h1>
      <ol class="list-group mb-3">
        <% @restaurants.each do |restaurant| %>
          <li class="list-grou-item"><%= link_to restaurant.name, restaurant_path(restaurant), class: "link" %></li>
          <% end %>
        </ol>
      <%= link_to "Add New Restaurant", new_restaurant_path %>
    </div>
  </div>
</div>
